Shader "Custom/aNewShader" {
	Properties {
		_MainTex ("Base (RGB)Base (RGB)", 2D) = "white" {}
		_MainTex2 ("haha", 2D) = "green" {}
		_EmissiveeeColor ("Emissiddve Color", Color) = (1,1,1,1)
	}
	SubShader {
		Tags { "RenderType"="Opaque" }
		LOD 200
		
		CGPROGRAM
		#pragma surface surf Lambert

		sampler2D _MainTex;

		struct Input {
			float2 uv_MainTex;
		};

		void surf (Input IN, inout SurfaceOutput o) {
			half4 c = tex2D (_MainTex, IN.uv_MainTex);
			o.Albedo = c.rgb;
			o.Alpha = c.a;
		}
		ENDCG
	} 
	FallBack "Diffuse"
}
